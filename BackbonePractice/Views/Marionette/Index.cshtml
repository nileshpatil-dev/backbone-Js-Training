
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="https://cdnjs.cloudflare.com/ajax/libs/handlebars.js/4.0.11/handlebars.min.js"></script>
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<div class="row"></div>
<div class="row">
    <div class="col-md-12">
        <div id="breadcrumbs">
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <div id="app"></div>
    </div>
</div>


<script id="index-template" type="text/x-backbone">
    <h1>Welcome to Marionette</h1>
    <ul>
        <li><a href="#" id="nav-user-list">Users</a></li>
        <li><a href="#">Roles</a></li>
    </ul>
</script>

<script id="user-list-template" type="text/x-backbone">
    <h1>Welcome to Marionette</h1>
    <ul>
        <li><a href="#" id="nav-user-list">Users</a></li>
        <li><a href="#">Roles</a></li>
    </ul>
</script>
<script id="user-detail-layout-template" type="text/x-backbone">
    <div class="row">
        <div id="summary-template" class="col-md-3 well">
        </div>
        <div id="detail-template" class="col-md-4">
        </div>
    </div>
</script>

<script id="user-summary-template" type="text/x-backbone">
    <img src="data:image/png;base64," alt="" />
    <h2><%= first %> <%= last %></h2>
    <ul>
        <li>Thing 1</li>
        <li>Thing 2</li>
    </ul>
</script>

<script id="user-detail-template" type="text/x-backbone">
    <a href="#" id="edit-user">Edit user</a>
</script>

<script id="user-edit-template" type="text/x-backbone">
    <form>
        <div class="form-group">
            <label for="first">First Name</label>
            <input type="text" class="form-control" name="first" placeholder="Enter first name" value="<%= first %>">
        </div>
        <div class="form-group">
            <label for="last">Last Name</label>
            <input type="text" class="form-control" name="last" placeholder="Enter last name" value="<%= last %>">
        </div>
        <div class="form-group">
            <label for="email">Email</label>
            <input type="email" class="form-control" name="email"  placeholder="Enter email" value="<%= email %>">
        </div>
        <button type="button" id="btnSaveUser" class="btn btn-primary">Save</button>
    </form>
</script>

<script src="~/Scripts/jquery-1.10.2.min.js"></script>

@Scripts.Render("~/bundle/vendor")
@Scripts.Render("~/bundle/user-admin")
<script>

    $(function () {
        UserAdmin.start();
    });


</script>
